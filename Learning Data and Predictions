  glm.accuracy <- mean(prediction ==test.set$most.frequent.class)
  featureless.predictions.list[[fold]] <- (predict(glm.model.))
  featureless.accuracy <- mean(featureless.)
  featureless.accuracies.list[[prediction]] <- featureless_accuracy

}


test.accuracy <- rbind(glm.accuracies.list, featureless.accuracies.list)
algorithm <- list("cv.glmnet", "featureless")
